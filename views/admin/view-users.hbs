


<div class="cbp-spmenu-push">
  <div class="main-content">
    {{>dash-header}}

    <div id="page-wrapper">
<section>
    <div class="container">
       
        <div class="pt-5" style="text-align:center">
            <h4>All Users</h4>
        </div>
         <br/>
        <table class="table mt-5" id="productListTable">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
      {{#each users}}
    <tr>
      <td scope="row">{{@index}}</td>
      <td>{{this.Name}}</td>
      <td>{{this.Email}}</td>
    </tr>
    {{/each}}
 
  </tbody>
</table>
    </div>
</section>

<script>
  $(document).ready( function () {
    $('#productListTable').DataTable();
  });
</script>

    
    </div>

  </div>

  {{>dash-footer}}
